<?xml version="1.0"?>
<config>
    <modules>
        <Chronopost_Chronorelais>
            <version>1.3.0</version>
            <depends>
                <Mage_Shipping />
                <Mage_Sales />
                <Mage_Adminhtml />
            </depends>
        </Chronopost_Chronorelais>
    </modules>

    <global>
        <models>
            <chronorelais>
                <class>Chronopost_Chronorelais_Model</class>
            </chronorelais>
        </models>
        <blocks>
            <chronorelais>
                <class>Chronopost_Chronorelais_Block</class>
            </chronorelais>
            <adminhtml>
                <rewrite>
                    <sales_order_shipment_view>Chronopost_Chronorelais_Block_Sales_Order_Shipment_View</sales_order_shipment_view>
                </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <chronorelais>
                <class>Chronopost_Chronorelais_Helper</class>
            </chronorelais>
        </helpers>

        <resources>
            <sales>
                <shipping>
                    <carriers>
                        <chronorelais>
                            <class>Chronopost_Chronorelais_Model_Carrier_Chronorelais</class>
                        </chronorelais>
                        <chronopost>
                            <class>Chronopost_Chronorelais_Model_Carrier_Chronopost</class>
                        </chronopost>
                        <chronoexpress>
                            <class>Chronopost_Chronorelais_Model_Carrier_Chronoexpress</class>
                        </chronoexpress>
                    </carriers>
                </shipping>
            </sales>
            <chronorelais_setup>
                <setup>
                    <module>Chronopost_Chronorelais</module>
                    <class>Mage_Catalog_Model_Resource_Eav_Mysql4_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </chronorelais_setup>
            <chronorelais_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </chronorelais_write>

            <chronorelais_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </chronorelais_read>
        </resources>

        <rewrite>
            <chronopost_chronorelais_checkout_multishipping>
                <from><![CDATA[#^/checkout/multishipping/#]]>
                </from>
                <to>/chronorelais/checkout_multishipping/</to>
            </chronopost_chronorelais_checkout_multishipping>
            <chronopost_chronorelais_sales_order_shipment>
                <from><![CDATA[#^/(.*)sales_order_shipment/save/#]]>
                </from>
                <to><![CDATA[/$1chronorelais_sales_order_shipment/save/]]></to>
            </chronopost_chronorelais_sales_order_shipment>
        </rewrite>

        <fieldsets>
            <sales_copy_order_billing_address>
                <w_relay_point_code>
                    <to_order>*</to_order>
                </w_relay_point_code>
            </sales_copy_order_billing_address>
            <sales_copy_order_shipping_address>
                <w_relay_point_code>
                    <to_order>*</to_order>
                </w_relay_point_code>
            </sales_copy_order_shipping_address>
            <sales_convert_quote_address>
                <w_relay_point_code>
                    <to_order_address>*</to_order_address>
                    <to_customer_address>*</to_customer_address>
                </w_relay_point_code>
                <chronopostsrdv_creneaux_info>
                    <to_order_address>*</to_order_address>
                    <to_customer_address>*</to_customer_address>
                    <to_order>*</to_order>
                </chronopostsrdv_creneaux_info>
            </sales_convert_quote_address>
            <sales_convert_order_address>
                <w_relay_point_code>
                    <to_quote_address>*</to_quote_address>
                </w_relay_point_code>
            </sales_convert_order_address>
            <customer_address>
                <w_relay_point_code>
                    <to_quote_address>*</to_quote_address>
                </w_relay_point_code>
            </customer_address>
        </fieldsets>

    </global>

    <frontend>
        <routers>
            <chronorelais>
                <use>standard</use>
                <args>
                    <module>Chronopost_Chronorelais</module>
                    <frontName>chronorelais</frontName>
                </args>
            </chronorelais>
        </routers>

        <layout>
            <updates>
                <chronorelais>
                    <file>chronorelais.xml</file>
                </chronorelais>
            </updates>
        </layout>
        <translate>
            <modules>
                <Chronopost_Chronorelais>
                    <files>
                        <default>Chronopost_Chronorelais.csv</default>
                    </files>
                </Chronopost_Chronorelais>
            </modules>
        </translate>

        <events>
            <controller_action_predispatch_checkout_onepage_index>
                <observers>
                    <check_soap_exists>
                        <class>chronorelais/observer</class>
                        <method>checkSoapExists</method>
                    </check_soap_exists>
                </observers>
            </controller_action_predispatch_checkout_onepage_index>

            <controller_action_predispatch_checkout_onepage_saveBilling>
                <observers>
                    <savebilling_before>
                        <class>chronorelais/observer</class>
                        <method>saveBillingBefore</method>
                    </savebilling_before>
                </observers>
            </controller_action_predispatch_checkout_onepage_saveBilling>

            <controller_action_predispatch_checkout_onepage_saveShipping>
                <observers>
                    <saveshipping_before>
                        <class>chronorelais/observer</class>
                        <method>saveShippingBefore</method>
                    </saveshipping_before>
                </observers>
            </controller_action_predispatch_checkout_onepage_saveShipping>

            <controller_action_predispatch_checkout_onepage_saveShippingMethod>
                <observers>
                    <saveshippingmethod_before>
                        <class>chronorelais/observer</class>
                        <method>saveShippingMethodBefore</method>
                    </saveshippingmethod_before>
                </observers>
            </controller_action_predispatch_checkout_onepage_saveShippingMethod>

            <controller_action_predispatch_onestepcheckout_ajax_set_methods_separate>
                <observers>
                    <saveshippingmethod_idev_onestepcheckout_before>
                        <class>chronorelais/observer</class>
                        <method>saveShippingMethodBefore</method>
                    </saveshippingmethod_idev_onestepcheckout_before>
                </observers>
            </controller_action_predispatch_onestepcheckout_ajax_set_methods_separate>

            <sales_order_place_before>
                <observers>
                    <check_relay_point>
                        <type>singleton</type>
                        <class>chronorelais/observer</class>
                        <method>checkRelayPoint</method>
                    </check_relay_point>
                </observers>
            </sales_order_place_before>

            <sales_convert_quote_address_to_order>
                <observers>
                    <update_order_with_relay_address>
                        <type>singleton</type>
                        <class>chronorelais/observer</class>
                        <method>updateShippingAddress</method>
                    </update_order_with_relay_address>
                </observers>
            </sales_convert_quote_address_to_order>

        </events>
    </frontend>

